{
  "name": "feathersjs-filter-ui",
  "version": "0.2.0",
  "author": "Adrian Jost <github@adrianjost.dev> (https://adrianjost.dev)",
  "scripts": {
    "build": "run-s build:*",
    "lint": "vue-cli-service lint",
    "deploy": "yarn lint && yarn build && yarn publish --access public",
    "build:docs": "vuepress build docs -d dist/docs",
    "build:lib": "vue-cli-service build --target lib --name FeathersjsFilterUi ./src/components/Filter.vue --mode production --dest dist/lib",
    "build:wc": "vue-cli-service build --target wc-async --name Feathersjs-filter-ui ./src/components/Filter.vue --mode production --dest dist/wc",
    "dev": "run-p dev:*",
    "dev:docs": "vuepress dev docs",
    "dev:vue": "vue-cli-service serve"
  },
  "dependencies": {
    "vue": "^2.6.10",
    "vue-my-dropdown": "^2.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.1",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21",
    "vuepress": "^0.14.11"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "files": [
    "dist/lib/*",
    "dist/wc/*"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "homepage": "https://adrianjost.github.io/feathersjs-filter-ui",
  "license": "MIT",
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "main": "dist/lib/FeathersjsFilterUi.umd.min.js",
  "module": "dist/lib/FeathersjsFilterUie.common.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": "github:adrianjost/feathersjs-filter-ui"
}
