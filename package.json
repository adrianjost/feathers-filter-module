{
  "name": "feathersjs-filter-ui",
  "version": "0.1.2",
  "author": "Adrian Jost <github@adrianjost.dev> (https://adrianjost.dev)",
  "repository": "github:adrianjost/feathersjs-filter-ui",
  "homepage": "https://adrianjost.github.io/feathersjs-filter-ui",
  "license": "MIT",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build:demo": "vue-cli-service build --mode production --dest dist/demo",
    "build:lib": "vue-cli-service build --target lib --name FeathersjsFilterUi ./src/components/Filter.vue --mode production --dest dist/lib",
    "build:wc": "vue-cli-service build --target wc --name Feathersjs-filter-ui ./src/components/Filter.vue --mode production --dest dist/wc",
    "build": "run-s build:*",
    "lint": "vue-cli-service lint",
    "deploy": "yarn lint && yarn build && yarn publish --access public"
  },
  "main": "dist/lib/FeathersjsFilterUi.umd.min.js",
  "module": "dist/lib/FeathersjsFilterUie.common.js",
  "files": [
    "dist/lib/*",
    "dist/wc/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10",
    "vue-material": "latest"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
